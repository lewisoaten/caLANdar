  "query": "\n        SELECT\n            email,\n            appid,\n            name,\n            playtime_forever,\n            user_game.last_modified\n        FROM user_game\n        JOIN steam_game USING(appid)\n        WHERE (appid = $1 OR $2)\n        AND (LOWER(email) = LOWER($3) OR $4)\n        ",
